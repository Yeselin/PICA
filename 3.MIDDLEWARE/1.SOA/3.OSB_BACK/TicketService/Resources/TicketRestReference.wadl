<?xml version = '1.0' encoding = 'UTF-8'?>
<application xmlns:soa="http://www.oracle.com/soa/rest" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:tns="http://tuBoleta/schema/ticketService" xmlns="http://wadl.dev.java.net/2009/02">
   <doc title="TicketRestReference">RestReference</doc>
   <grammars>
      <xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
            <xsd:import namespace="http://tuBoleta/schema/ticketService" schemaLocation="../Schemas/xsd/Ticket.xsd"/>
        </xsd:schema>
   </grammars>
   <resources>
      <resource path="/tuboleta/reserve">
         <method name="POST" soa:wsdlOperation="Reserve">
            <request>
               <representation mediaType="application/json" element="cns:createTicketRequest" xmlns:cns="http://tuBoleta/schema/ticketService"/>
            </request>
            <response status="200">
               <representation mediaType="application/json" element="cns:createTicketResponse" xmlns:cns="http://tuBoleta/schema/ticketService"/>
            </response>
         </method>
      </resource>
   </resources>
</application>
